{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "SudokuSpice/**.csproj"
          ]
        }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**/*.md",
          "api/**/*.yml"
        ]
      },
      { 
        "files": [ 
          "articles/*.md",
          "articles/*.yml"
        ]
      },
      { 
        "files": [ 
          "*.md",
          "*.yml"
        ],
        "exclude": "README.md"
      } 
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "dest": "docs",
    "globalMetadataFiles": ["globalmetadata.json"],
    "fileMetadataFiles": [],
    "template": [
      "default"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}

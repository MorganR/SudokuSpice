{
  "metadata": [
    {
      "src": [
        {
          "files": "SudokuSpice.csproj",
          "src": "../SudokuSpice"
        }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
       {
          "files": [
            "**.md",
            "**.yml",
            "toc.yml",
            "index.md"
          ],
          "src": "releases/v3.2.0",
          "dest": "releases/v3.2.0",
          "rootTocPath": "releases/v3.2.0",
          "version": "v3.2.0"
       },
       {
         "files": [
           "**.md",
           "**.yml",
           "toc.yml",
           "index.md"
         ],
         "src": "releases/v3.1.0",
         "dest": "releases/v3.1.0",
         "rootTocPath": "releases/v3.1.0",
         "version": "v3.1.0"
      },
      {
        "files": [
          "**.md",
          "**.yml",
          "toc.yml",
          "index.md"
        ],
        "src": "releases/v3.0.0",
        "dest": "releases/v3.0.0",
        "rootTocPath": "releases/v3.0.0",
        "version": "v3.0.0"
      },
      {
        "files": [
          "**.md",
          "**.yml",
          "toc.yml",
          "index.md"
        ],
        "src": "releases/v2.0.0",
        "dest": "releases/v2.0.0",
        "rootTocPath": "releases/v2.0.0",
        "version": "v2.0.0"
      },
      {
        "files": [
          "api/**/*.md",
          "api/**/*.yml"
        ]
      },
      {
        "files": [
          "articles/*.md",
          "articles/*.yml"
        ]
      },
      {
        "files": [
          "releases/*.md",
          "releases/*.yml"
        ]
      },
      {
        "files": [
          "*.md",
          "*.yml"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "dest": "../docs",
    "globalMetadataFiles": [ "globalmetadata.json" ],
    "fileMetadataFiles": [],
    "template": [
      "default"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}
